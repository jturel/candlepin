<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">
  <changeSet id="20160518135249-1" author="fnguyen">
    <comment> suspend-mode</comment>
    <createTable tableName="cp_mode">
      <column name="id" type="VARCHAR(32)">
        <constraints nullable="false" primaryKey="true" primaryKeyName="cp_mode_key_pkey"/>
      </column>
      <column name="created" type="TIMESTAMP "/>
      <column name="updated" type="TIMESTAMP "/>
      <column name="reason" type="VARCHAR(255)">
        <constraints nullable="false"/>
      </column>
      <column name="mode" type="VARCHAR(16)">
        <constraints nullable="false"/>
      </column>
      <column name="changeTime" type="TIMESTAMP"/>
    </createTable>


     <createIndex indexName="idx_mode_changeTime" tableName="cp_mode" unique="false">
       <column name="changeTime"/>
     </createIndex>

  </changeSet>
</databaseChangeLog>
